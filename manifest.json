{
  "manifest_version": 2,
"icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "name": "Snow Overlay",
  "version": "0.1",
"web_accessible_resources": ["script.js"] ,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
]
}
